const StyledTab = styled(Tab)(({ theme }) => {
  const backgroundColor =
    theme.palette.mode === "light"
      ? theme.palette.grey[100]
      : theme.palette.grey[800];
  return {
    height: theme.spacing(3),
    fontSize: "1rem",
    cursor: "pointer",
    // color: "#009fda !important",
    fontWeight: theme.typography.fontWeightBold,
    // padding: "12px 16px 12px 0",
    "&:hover, &:focus": {
      // color: "#009fda !important",
      // backgroundColor: emphasize(backgroundColor, 0.06),
    },
    "&:active": {
      boxShadow: theme.shadows[1],
      color: "#0075b0 !important",
    },
  };
});

const seletedStyle = {
  border: "1px solid #0473ea",
  color: "#0473ea",
};

const normalStyle = {
  border: "1px solid #6c757d",
  color: "#6c757d",
};

export default function Dashboard() {
  const [showReport, setShowReport] = useState(false);
  const [selectedTab, setSelectedTab] = useState("recents");
  const [showPopper, setShowPopper] = useState(false);

  const [filteredReports, setFilteredReports] = useState(reportData);

  const [anchorEl, setAnchorEl] = useState(null);
  const [loader, setLoader] = useState(true);

  const [showProfile, setShowProfile] = useState(false);

  const showProfileHandler = () => {
    setShowProfile(true);
  };

  const hideProfileHandler = () => {
    setShowProfile(false);
  };

  const [selectionRange, setSelectionRange] = useState({
    startDate: new Date(),
    endDate: new Date(),
    key: "selection",
  });

  const id = showPopper ? "date-popover" : undefined;

  useEffect(() => {
    setTimeout(() => {
      setLoader(false);
    }, 4500);
  }, []);

  const openReportView = () => {
    setShowReport(true);
  };

  const closeReportView = () => {
    setShowReport(false);
  };

  const handleTabChange = (event, value) => {
    setSelectedTab(value);
  };

  const showDateRange = (event) => {
    setAnchorEl(event.currentTarget);
    setShowPopper((prev) => !prev);
  };

  const handleSelectRange = (ranges) => {
    setSelectionRange(ranges.selection);
  };

  return (
    <>
      <Profile open={showProfile} closeAction={hideProfileHandler} />
      <Navbar
        showProfile={showProfileHandler}
        // closeProfile={hideProfileHandler}
      />
      {!showReport ? (
        <div style={{ padding: "24px" }}>
          {/* <p
            className="dashboard-headings"
            style={{ marginBottom: "2rem", marginTop: "1rem" }}
          >
            My Reports
          </p> */}
          <Tabs
            value={selectedTab}
            onChange={handleTabChange}
            textColor="secondary"
            indicatorColor="secondary"
            aria-label="secondary tabs example"
            style={{ marginBottom: "1.5rem" }}
          >
            <StyledTab value="recents" label="Recents" style={{}} />
            <StyledTab value="favourites" label="Favourites" />
          </Tabs>

          <div style={{ display: "flex", marginBottom: "4rem" }}>
            {loader ? (
              <>
                <Skeleton
                  variant="rectangular"
                  width={300}
                  height={300}
                  style={{ marginRight: "1rem" }}
                />
                <Skeleton
                  variant="rectangular"
                  width={300}
                  height={300}
                  style={{ marginRight: "1rem" }}
                />
                <Skeleton
                  variant="rectangular"
                  width={300}
                  height={300}
                  style={{ marginRight: "1rem" }}
                />
                <Skeleton
                  variant="rectangular"
                  width={300}
                  height={300}
                  style={{ marginRight: "1rem" }}
                />
              </>
            ) : (
              <>
                {reportData.map((report) =>
                  report.tags.includes(selectedTab) ? (
                    <Zoom in={true} {...(true ? { timeout: 500 } : {})}>
                      <div
                        style={{
                          marginRight: "1rem",
                          width: "300px",
                          height: "300px",
                        }}
                      >
                        <MediaCard
                          openAction={openReportView}
                          report={report}
                        />
                      </div>
                    </Zoom>
                  ) : (
                    ""
                  )
                )}
              </>
            )}
          </div>

          <p className="dashboard-headings" style={{ marginTop: "2rem" }}>
            Folders
          </p>
          <div style={{ display: "flex", marginBottom: "4rem" }}>
            {loader ? (
              <>
                <Skeleton
                  variant="rectangular"
                  width={"10rem"}
                  height={"5rem"}
                  style={{ marginRight: "1rem" }}
                />
                <Skeleton
                  variant="rectangular"
                  width={"10rem"}
                  height={"5rem"}
                  style={{ marginRight: "1rem" }}
                />
                <Skeleton
                  variant="rectangular"
                  width={"10rem"}
                  height={"5rem"}
                  style={{ marginRight: "1rem" }}
                />
              </>
            ) : (
              <>
                {folderData.map((folder) => {
                  return (
                    <Zoom in={true} {...(true ? { timeout: 500 } : {})}>
                      <Card
                        variant="outlined"
                        style={{
                          padding: "1.5rem",
                          marginRight: "0.75rem",
                          minWidth: "10rem",
                          backgroundColor: "white",
                          boxShadow: "0 0 10px lightgray",
                          border: "none",
                          cursor: "pointer",
                        }}
                      >
                        <div style={{ display: "flex" }}>
                          <FolderIcon
                            style={{ color: "#6D6E71", marginRight: "0.25rem" }}
                          />
                          <p style={{ margin: "0" }}>{folder}</p>
                        </div>
                      </Card>
                    </Zoom>
                  );
                })}
              </>
            )}
          </div>

          <section>
            <p
              className="dashboard-headings"
              style={{ marginTop: "2rem", fontSize: "1.2rem" }}
            >
              My Reports
            </p>
          </section>

          <>
            <div
              style={{
                display: "flex",
                // justifyContent: "flex-end",
                marginBottom: "1rem",
              }}
            >
              <Button
                aria-describedby={id}
                variant="outlined"
                onClick={showDateRange}
                style={normalStyle}
              >
                Date <ArrowDropDownIcon />
              </Button>
              <Popover
                id={id}
                open={showPopper}
                anchorEl={anchorEl}
                onClose={showDateRange}
                anchorOrigin={{
                  vertical: "top",
                  horizontal: "right",
                }}
              >
                <DateRange
                  ranges={[selectionRange]}
                  onChange={handleSelectRange}
                />
              </Popover>

              <FormControl style={{ width: 100, marginLeft: "1rem" }}>
                <InputLabel id="filter-type">Type</InputLabel>
                <Select
                  labelId="filter-type"
                  id="filter-type-select"
                  // value={age}
                  label="Type"
                  // onChange={handleChange}
                >
                  <MenuItem value="PDF">PDF</MenuItem>
                  <MenuItem value="XML">XML</MenuItem>
                </Select>
              </FormControl>
            </div>
          </>
          {loader ? (
            <Skeleton
              variant="rectangular"
              width={"100%"}
              height={"20rem"}
              style={{ marginRight: "1rem" }}
            />
          ) : (
            <Table data={filteredReports} openAction={openReportView} />
          )}
        </div>
      ) : (
        <ReportViewer closeAction={closeReportView} />
      )}
    </>
  );
}
import { useEffect, useState } from "react";
import { useHistory } from "react-router-dom/cjs/react-router-dom";
import "./login.css";
import {
  loadCaptchaEnginge,
  LoadCanvasTemplate,
  LoadCanvasTemplateNoReload,
  validateCaptcha,
} from "react-simple-captcha";

import { TextField, Button, Tooltip, LinearProgress } from "@mui/material";
import { styled } from "@material-ui/core";

import PersonIcon from "@mui/icons-material/Person";

const CssTextField = styled(TextField)({
  "& label.Mui-focused": {
    color: "#3F9C35",
  },
  "& .MuiOutlinedInput-root": {
    marginBottom: "1rem",
    backgroundColor: "white",
    "& fieldset": {
      // boxShadow: "0 0 10px lightgray",
      border: "1px solid #6D6E71",
    },
    "&:hover fieldset": {
      borderColor: "#6D6E71",
    },
    "&.Mui-focused fieldset": {
      border: "2px solid #3F9C35",
      boxShadow: "none",
    },
  },
});

function Login() {
  const [userData, setUserData] = useState({ email: "", password: "" });
  const [formState, setFormState] = useState("email");
  const [localData, setLocalData] = useState({});
  const [loader, setLoader] = useState(false);

  const history = useHistory();

  useEffect(() => {
    if (formState === "password") {
      loadCaptchaEnginge(6, "rgba(255, 255, 255, 0)");
      // style fix for catptcha on initial render
      let canvas = document.getElementById("canv");
      canvas.style.height = "30px";
      canvas.parentNode.style.display = "flex";
      canvas.parentNode.style.alignItems = "center";
      canvas.parentNode.style.justifyContent = "center";
    }

    // fetch from localstorage and setlocalstate and userData
  }, [formState]);

  const handleChange = (event) => {
    setUserData({ ...userData, [event.target.name]: event.target.value });
  };

  const nextFormState = (event) => {
    setLoader(true);
    setTimeout(() => {
      switch (formState) {
        case "email":
          setFormState("password");
          break;
        case "password":
          history.push("/dashboard");
          break;
        default:
          setFormState("email");
      }
      setLoader(false);
    }, 2000);
  };

  const previousFormState = (event) => {
    switch (formState) {
      default:
        setUserData({ ...userData, password: "" });
        setFormState("email");
    }
  };

  return (
    <div className="login-container">
      <div className="login-wrapper">
        {loader ? <LinearProgress color="success" /> : ""}
        <img
          src="https://logowik.com/content/uploads/images/standard-chartered-bank-new-20211713.jpg"
          id="login-logo-image"
        />

        <form id="login-form">
          {formState === "email" ? (
            <>
              <p className="login-form-header">Welcome Back!</p>{" "}
              {localData?.email ? (
                <div style={{ display: "flex", margin: "0rem 0rem 1rem 0" }}>
                  <PersonIcon style={{ marginRight: "0.5rem" }} />
                  <p style={{ margin: "0" }}>{localData.email}</p>
                </div>
              ) : (
                <CssTextField
                  variant="outlined"
                  label="Email"
                  name="email"
                  value={userData.email}
                  required
                  placeholder="Enter Email"
                  onChange={handleChange}
                />
              )}
            </>
          ) : (
            <>
              <p className="account-header">
                <PersonIcon style={{ margin: "0 0.25rem 0 0" }} />
                {userData.email}
              </p>
              <CssTextField
                variant="outlined"
                label="Password"
                name="password"
                type="password"
                required
                placeholder="Enter Password"
                onChange={handleChange}
              />
              <p id="forgot-password">Forgot Password ?</p>
              <div id="captcha-wrapper">
                <CssTextField
                  variant="outlined"
                  label="Captcha"
                  required
                  placeholder="Enter Captcha"
                />
                <LoadCanvasTemplate reloadText="<img alt='reload' id='reload-icon' src='https://upload.wikimedia.org/wikipedia/commons/thumb/7/7d/Refresh_icon.svg/2048px-Refresh_icon.svg.png'/>" />
              </div>
            </>
          )}

          <div
            style={{
              display: "flex",
              justifyContent: "flex-end",
              marginTop: "0.75rem",
            }}
          >
            {formState !== "email" ? (
              <Button
                variant="contained"
                style={{ backgroundColor: "#939598", marginRight: "auto" }}
                onClick={previousFormState}
              >
                Back
              </Button>
            ) : (
              ""
            )}

            <Button
              variant="contained"
              style={{ backgroundColor: "#3F9C35" }}
              onClick={nextFormState}
              disabled={loader}
            >
              {formState === "password" ? "Login" : "Next"}
            </Button>
          </div>
        </form>
      </div>
    </div>
  );
}

export default Login;
