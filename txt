{breadcrumbs.map((breadcrumb, index) => (
            <Typography
              variant="body1"
              color="textSecondary"
              key={breadcrumb.name}
            >
              {index > 0 && ">"}
              {index === breadcrumbs.length - 1 ? (
                <span>{breadcrumb.name}</span>
              ) : (
                <a href={breadcrumb.path}>{breadcrumb.name}</a>
              )}
            </Typography>
          ))}
