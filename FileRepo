package com.scb.axessspringboottraining.repo;

import java.util.Date;
import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.scb.axessspringboottraining.entity.File;
import com.scb.axessspringboottraining.entity.FolderPath;

@Repository
public interface FileRepository extends JpaRepository<File, Long>{
	List<File> getFilesByFolderPath(FolderPath folderPath);
	List<File> findByFolderPathAndDateUploadedBetween(FolderPath folderpath, Date startDate, Date endDate);
//	List<File> findAllFilesByUploadDate(Date uploadDate);
//
//    List<File> findAllFilesByUploadTimeBetween(
//      Date UploadTimeStart,
//      Date UploadTimeEnd);
}
